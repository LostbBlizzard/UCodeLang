//import ULang::{Span,StringSpan,Console};
use ULang;


|main[]:
   Fmt::Println("A Mini Linux Terminal Emulator");
   Fmt::Println("Available commands:");
   Fmt::Println("cd    //Change Directory");
   Fmt::Println("clear //Clear terminal screen");
   Fmt::Println("echo  //Display message on screen");
   Fmt::Println("cat   //Display file on screen");
   Fmt::Println("cp    //Copy");
   Fmt::Println("rm    //Remove file");
   
   while true;